<v:View x:Class="CegsMines.Views.AirManifold1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:CegsMines.Views"
        xmlns:v="clr-namespace:AeonHacs.Wpf.Views;assembly=AeonHacs.Wpf"
        mc:Ignorable="d">
    <Border Background="Transparent" AutomationProperties.HelpText="Air Solenoid Manifolds">
        <StackPanel>
            <Label Content="Air Manifold 1" HorizontalAlignment="Center" FontSize="10" Margin="0, 10, 0, 0" Padding="0" />
            <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center">
                <StackPanel.Resources>
                    <Style TargetType="{x:Type v:Valve}">
                        <Setter Property="Margin" Value="2" />
                    </Style>
                </StackPanel.Resources>
                <v:Valve HelpText="Coil Trap 1 Coldfinger Air Valve" Component="vAir_CT1" />
                <v:Valve HelpText="Coil Trap 2 Coldfinger Air Valve" Component="vAir_CT2" />
                <v:Valve HelpText="Spare Air Valve 1" Component="vAir_Spare1" />
                <v:Valve HelpText="Spare Air Valve 2" Component="vAir_Spare2" />
                <v:Valve HelpText="Variable Temperature Coldfinger Air Valve" Component="vAir_VTC" />
                <v:Valve HelpText="GR1 Coldfinger Air Valve" Component="vAir_GR1" />
                <v:Valve HelpText="GR2 Coldfinger Air Valve" Component="vAir_GR2" />
                <v:Valve HelpText="GR3 Coldfinger Air Valve" Component="vAir_GR3" />
                <v:Valve HelpText="Measurement Chamber Coldfinger Air Valve" Component="vAir_MC" />
            </StackPanel>
        </StackPanel>
    </Border>
</v:View>
